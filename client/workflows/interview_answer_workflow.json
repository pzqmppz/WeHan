            "code": "async function main({ params }: Args): Promise<Output> {\n  const llmOutput = params.llmOutput;\n  const interviewData = params.interviewData.data || params.interviewData;\n  const questionIndex = parseInt(params.questionIndex);\n  \n  let evaluation = { score: 5, feedback: \"回答收到了\", highlights: \"\", improvements: \"\" };\n  try {\n    let jsonStr = llmOutput;\n    const jsonMatch = llmOutput.match(/\\{[\\s\\S]*\\}/);\n    if (jsonMatch) {\n      jsonStr = jsonMatch[0];\n    }\n    evaluation = JSON.parse(jsonStr);\n  } catch (e) {\n    // 解析失败使用默认值\n  }\n  \n  const outline = interviewData.outline || [];\n  const currentQuestion = outline[questionIndex] || {};\n  \n  // 构建答案记录\n  const answerRecord = {\n    questionIndex: questionIndex,\n    question: currentQuestion.question || \"题目\",\n    answer: params.answer || \"用户回答\",\n    score: evaluation.score,\n    feedback: evaluation.feedback,\n    timestamp: new Date().toISOString()\n  };\n  \n  // 获取下一题\n  const nextIndex = questionIndex + 1;\n  const nextQuestion = outline[nextIndex] || null;\n  \n  return {\n    score: evaluation.score,\n    feedback: evaluation.feedback,\n    answerRecord: answerRecord,\n    nextQuestion: nextQuestion ? nextQuestion.question : \"\",\n    nextIndex: nextIndex,\n    isCompleted: !nextQuestion,\n    totalQuestions: outline.length,\n    progress: `${Math.min(nextIndex, outline.length)}/${outline.length}`,\n    currentQuestionTitle: currentQuestion.question || \"\"\n  };\n}",
